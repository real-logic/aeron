        <field name="logPosition"              id="2" type="int64"/>
        <field name="timestamp"                id="3" type="time_t"/>
        <field name="action"                   id="4" type="ClusterAction"/>
        <field name="flags"                    id="5" type="int32" sinceVersion="11" presence="optional"/>
    </sbe:message>

